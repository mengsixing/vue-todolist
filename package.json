{
  "name": "vue-todolist",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --ext js --ext jsx --ext vue ./client",
    "lint-fix": "eslint --fix --ext js --ext jsx --ext vue ./client",
    "prebuild": "rimraf ./public ./server-build",
    "build": "cross-env NODE_ENV=production webpack --config ./build/webpack.config.client.js && cross-env NODE_ENV=production webpack --config ./build/webpack.config.server.js",
    "practice": "cross-env NODE_ENV=development webpack-dev-server --config ./build/webpack.config.practive.js",
    "dev:client": "cross-env NODE_ENV=development webpack-dev-server --config ./build/webpack.config.client.js",
    "dev:server": "nodemon server/server.js",
    "dev": "yarn run dev:client & yarn run dev:server",
    "build:client": "cross-env NODE_ENV=production webpack --config ./build/webpack.config.client.js",
    "build:server": "cross-env NODE_ENV=production webpack --config ./build/webpack.config.server.js",
    "start": "nodemon server/server.js"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yhlben/vue-todolist.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/yhlben/vue-todolist/issues"
  },
  "homepage": "https://github.com/yhlben/vue-todolist#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "ejs": "^2.6.1",
    "koa": "^2.5.1",
    "koa-router": "^7.4.0",
    "koa-send": "^5.0.0",
    "vue": "^2.5.16",
    "vue-router": "^3.0.1",
    "vue-server-renderer": "^2.5.16",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^8.6.2",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.24.1",
    "cross-env": "^5.1.6",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-html": "^4.0.3",
    "eslint-plugin-vue": "^4.5.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "memory-fs": "^0.4.1",
    "mini-css-extract-plugin": "^0.4.0",
    "nodemon": "^1.17.5",
    "postcss-loader": "^2.1.5",
    "rimraf": "^2.6.2",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "vue-loader": "^15.2.4",
    "vue-style-loader": "^4.1.0",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.3",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.3"
  }
}
